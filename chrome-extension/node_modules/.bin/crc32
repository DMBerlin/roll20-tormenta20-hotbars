#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/proc/cygdrive/c/Users/danie/Documents/.tampermonkey-scripts/roll20/chrome-extension/node_modules/crc-32/bin/node_modules:/proc/cygdrive/c/Users/danie/Documents/.tampermonkey-scripts/roll20/chrome-extension/node_modules/crc-32/node_modules:/proc/cygdrive/c/Users/danie/Documents/.tampermonkey-scripts/roll20/chrome-extension/node_modules:/proc/cygdrive/c/Users/danie/Documents/.tampermonkey-scripts/roll20/node_modules:/proc/cygdrive/c/Users/danie/Documents/.tampermonkey-scripts/node_modules:/proc/cygdrive/c/Users/danie/Documents/node_modules:/proc/cygdrive/c/Users/danie/node_modules:/proc/cygdrive/c/Users/node_modules:/proc/cygdrive/c/node_modules:/proc/cygdrive/c/Users/danie/Documents/.tampermonkey-scripts/roll20/chrome-extension/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/proc/cygdrive/c/Users/danie/Documents/.tampermonkey-scripts/roll20/chrome-extension/node_modules/crc-32/bin/node_modules:/proc/cygdrive/c/Users/danie/Documents/.tampermonkey-scripts/roll20/chrome-extension/node_modules/crc-32/node_modules:/proc/cygdrive/c/Users/danie/Documents/.tampermonkey-scripts/roll20/chrome-extension/node_modules:/proc/cygdrive/c/Users/danie/Documents/.tampermonkey-scripts/roll20/node_modules:/proc/cygdrive/c/Users/danie/Documents/.tampermonkey-scripts/node_modules:/proc/cygdrive/c/Users/danie/Documents/node_modules:/proc/cygdrive/c/Users/danie/node_modules:/proc/cygdrive/c/Users/node_modules:/proc/cygdrive/c/node_modules:/proc/cygdrive/c/Users/danie/Documents/.tampermonkey-scripts/roll20/chrome-extension/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../crc-32/bin/crc32.njs" "$@"
else
  exec node  "$basedir/../crc-32/bin/crc32.njs" "$@"
fi
